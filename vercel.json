{
  "version": 2,
  "builds": [
    {
      "src": "/",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "/"
      }
    },
    {
      "use": "@vercel/python",
      "includeFiles": "/**",
      "config": {
        "maxLambdaSize": "50mb",
        "cmd": "python3 run.py",
        "requirements": "requirements.txt"
      }
    }
  ]
}

